{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "globals": {
    "google": true
  },
  "rules": {
    "semi": [ 2, "always" ],
    "quotes": [ 1, "single" ],
    "strict": 0,
    "no-undef": 1,
    "no-unused-vars": [1, {"vars": "all", "args": "after-used", "ignore": ["React"]}],
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/jsx-no-undef": 1,
    "react/react-in-jsx-scope": 1
  },
  "plugins": [
    "react"
  ]
}